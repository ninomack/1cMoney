    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	Статистика.Ссылка КАК Ссылка,
	Статистика.КоличествоНеактуальных + Статистика.КоличествоАктуальных - 1 КАК КоличествоПодчиненных
ПОМЕСТИТЬ СтатьиОтбора
ИЗ
	РегистрСведений.СтатистикаСправочников КАК Статистика
ГДЕ
	Статистика.Ссылка ССЫЛКА Справочник.СтатьиДоходов
	И (&amp;ОтборПоАктивности = 0
			ИЛИ &amp;ОтборПоАктивности > 0
				И Статистика.КоличествоАктуальных > 0
			ИЛИ &amp;ОтборПоАктивности &lt; 0
				И Статистика.КоличествоНеактуальных > 0)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Статьи.Ссылка КАК Ссылка,
	Статьи.ВерсияДанных КАК ВерсияДанных,
	Статьи.ПометкаУдаления КАК ПометкаУдаления,
	Статьи.Родитель КАК Родитель,
	Статьи.Наименование КАК Наименование,
	Статьи.Активность КАК Активность,
	Статьи.Комментарий КАК Комментарий,
	Статьи.УдалитьПропускатьВводКомментария КАК УдалитьПропускатьВводКомментария,
	Статьи.УдалитьКомментарийПоУмолчанию КАК УдалитьКомментарийПоУмолчанию,
	Статьи.ДополнительныеРеквизиты.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Свойство КАК Свойство,
		Значение КАК Значение,
		ТекстоваяСтрока КАК ТекстоваяСтрока
	) КАК ДополнительныеРеквизиты,
	Статьи.Предопределенный КАК Предопределенный,
	Статьи.ИмяПредопределенныхДанных КАК ИмяПредопределенныхДанных,
	ВЫБОР
		КОГДА Статистика.КоличествоПодчиненных &lt;> 0
			ТОГДА 0
		ИНАЧЕ 3
	КОНЕЦ + ВЫБОР
		КОГДА Статьи.ПометкаУдаления
			ТОГДА 1
		КОГДА Статьи.Предопределенный
			ТОГДА 2
		ИНАЧЕ 0
	КОНЕЦ КАК ИндексКартинки,
	Статистика.КоличествоПодчиненных КАК КоличествоПодчиненных
{ВЫБРАТЬ
	Ссылка.*,
	ПометкаУдаления,
	Родитель.*,
	Наименование,
	Активность,
	Комментарий,
	ИндексКартинки,
	КоличествоПодчиненных}
ИЗ
	Справочник.СтатьиДоходов КАК Статьи
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ СтатьиОтбора КАК Статистика
		ПО (Статистика.Ссылка = Статьи.Ссылка)
{ГДЕ
	Статьи.Ссылка.*,
	Статьи.Родитель.*,
	Статьи.Наименование,
	Статьи.Активность,
	Статьи.Комментарий,
	Статистика.КоличествоПодчиненных,
	Статьи.ПометкаУдаления}	</queryText>
      <mainTable>Catalog.СтатьиДоходов</mainTable>
