		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	0 КАК Цифра
ПОМЕСТИТЬ ТаблицаЦифр

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	1

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	2

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	3

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	4

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	5

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	6

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	7

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	8

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	9
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТаблицаЦифр1.Цифра + ТаблицаЦифр10.Цифра * 10 + ТаблицаЦифр100.Цифра * 100 + ТаблицаЦифр1000.Цифра * 1000 КАК Номер
ПОМЕСТИТЬ ТаблицаНомеров
ИЗ
	ТаблицаЦифр КАК ТаблицаЦифр1,
	ТаблицаЦифр КАК ТаблицаЦифр10,
	ТаблицаЦифр КАК ТаблицаЦифр100,
	ТаблицаЦифр КАК ТаблицаЦифр1000
ГДЕ
	ТаблицаЦифр1.Цифра + ТаблицаЦифр10.Цифра * 10 + ТаблицаЦифр100.Цифра * 100 + ТаблицаЦифр1000.Цифра * 1000 &lt;= РАЗНОСТЬДАТ(&amp;ДатаОтбора1, &amp;ДатаОтбора2, ДЕНЬ) / 7

ИНДЕКСИРОВАТЬ ПО
	Номер
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Периоды.НомерПериода КАК НомерПериода,
	Периоды.НачалоПериода КАК НачалоПериода,
	Периоды.ОкончаниеПериода КАК ОкончаниеПериода
ПОМЕСТИТЬ ТаблицаПериодов
ИЗ
	(ВЫБРАТЬ
		ТаблицаНомеров.Номер + 1 КАК НомерПериода,
		ВЫБОР
			КОГДА &amp;ВидПериода = "НЕДЕЛЯ"
				ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, НЕДЕЛЯ, ТаблицаНомеров.Номер)
			КОГДА &amp;ВидПериода = "МЕСЯЦ"
				ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, МЕСЯЦ, ТаблицаНомеров.Номер)
			КОГДА &amp;ВидПериода = "КВАРТАЛ"
				ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, КВАРТАЛ, ТаблицаНомеров.Номер)
			КОГДА &amp;ВидПериода = "ГОД"
				ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, ГОД, ТаблицаНомеров.Номер)
		КОНЕЦ КАК НачалоПериода,
		ДОБАВИТЬКДАТЕ(ВЫБОР
				КОГДА &amp;ВидПериода = "НЕДЕЛЯ"
					ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, НЕДЕЛЯ, ТаблицаНомеров.Номер + 1)
				КОГДА &amp;ВидПериода = "МЕСЯЦ"
					ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, МЕСЯЦ, ТаблицаНомеров.Номер + 1)
				КОГДА &amp;ВидПериода = "КВАРТАЛ"
					ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, КВАРТАЛ, ТаблицаНомеров.Номер + 1)
				КОГДА &amp;ВидПериода = "ГОД"
					ТОГДА ДОБАВИТЬКДАТЕ(&amp;ДатаОтбора1, ГОД, ТаблицаНомеров.Номер + 1)
			КОНЕЦ, СЕКУНДА, -1) КАК ОкончаниеПериода
	ИЗ
		ТаблицаНомеров КАК ТаблицаНомеров) КАК Периоды
ГДЕ
	Периоды.НачалоПериода &gt;= &amp;ДатаОтбора1
	И Периоды.ОкончаниеПериода &lt;= &amp;ДатаОтбора2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	КурсыВалютСрезПоследних.Валюта,
	ВЫБОР
		КОГДА КурсыВалютСрезПоследних.Курс = 0
			ТОГДА 1
		ИНАЧЕ КурсыВалютСрезПоследних.Курс
	КОНЕЦ КАК Курс,
	ВЫБОР
		КОГДА КурсыВалютСрезПоследних.Кратность = 0
			ТОГДА 1
		ИНАЧЕ КурсыВалютСрезПоследних.Кратность
	КОНЕЦ КАК Кратность
ПОМЕСТИТЬ ПоследниеКурсы
ИЗ
	РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаОтбора2, БазоваяВалюта = &amp;ВалютаУчета) КАК КурсыВалютСрезПоследних
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТаблицаПериодов.НомерПериода,
	жОстаткиИОбороты.ПериодДень КАК День,
	жОстаткиИОбороты.Регистратор КАК Операция,
	жОстаткиИОбороты.Валюта КАК Валюта,
	ВЫРАЗИТЬ(ЕСТЬNULL(КурсыВалют.Курс, ЕСТЬNULL(ПоследниеКурсы.Курс, 0)) * ЕСТЬNULL(КурсыВалютыОтчета.Кратность, ЕСТЬNULL(ПоследниеКурсыОтчета.Кратность, 0)) / ВЫБОР
			КОГДА ЕСТЬNULL(КурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(КурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1)) = 0
				ТОГДА 1
			ИНАЧЕ ЕСТЬNULL(КурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(КурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1))
		КОНЕЦ КАК ЧИСЛО(15, 8)) КАК Курс,
	жОстаткиИОбороты.ВалютнаяСуммаНачальныйОстаток КАК НаНачалоВВалютеКошелька,
	жОстаткиИОбороты.ВалютнаяСуммаОборотДт КАК ПриходВВалютеКошелька,
	жОстаткиИОбороты.ВалютнаяСуммаОборотКт КАК РасходВВалютеКошелька,
	жОстаткиИОбороты.ВалютнаяСуммаКонечныйОстаток КАК НаКонецВВалютеКошелька,
	ВЫРАЗИТЬ(ЕСТЬNULL(жОстаткиИОбороты.Субконто2, НЕОПРЕДЕЛЕНО) КАК Справочник.ФинансовыеЦели) КАК ФинансоваяЦель,
	жОстаткиИОбороты.Счет КАК ВидДенег,
	ВЫБОР
		КОГДА ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета).ТипСчета = ЗНАЧЕНИЕ(Перечисление.ТипыСчетов.БанковскаяКартаКредитная)
			ТОГДА &amp;Синоним_КредитныеКарты
		КОГДА ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета).ТипСчета = ЗНАЧЕНИЕ(Перечисление.ТипыСчетов.БанковскаяКартаУниверсальная)
			И (жОстаткиИОбороты.ВалютнаяСуммаКонечныйОстаток &lt; 0 ИЛИ жОстаткиИОбороты.ВалютнаяСуммаНачальныйОстаток &lt; 0)
			ТОГДА &amp;Синоним_КредитныеКарты
		ИНАЧЕ &amp;Синоним_ДеньгиВНаличии
	КОНЕЦ КАК ТипКошелька,
	ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета) КАК Кошелек,
	ВЫРАЗИТЬ(жОстаткиИОбороты.Регистратор.ОписаниеОперации КАК СТРОКА(1000)) КАК ОписаниеОперации,
	жОстаткиИОбороты.Валюта.Наименование КАК СимволВалюты,
	ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета).ЭтоГруппа КАК ЭтоГруппаКошельков,
	ВЫБОР 
		КОГДА жОстаткиИОбороты.ВалютнаяСуммаОборот &lt;&gt; 0 И жОстаткиИОбороты.ПериодСекунда = НАЧАЛОПЕРИОДА(жОстаткиИОбороты.ПериодСекунда, ДЕНЬ) 
			ТОГДА ДОБАВИТЬКДАТЕ(жОстаткиИОбороты.ПериодСекунда, СЕКУНДА, 2)
		КОГДА жОстаткиИОбороты.ВалютнаяСуммаОборот &lt;&gt; 0 И жОстаткиИОбороты.ПериодСекунда = КОНЕЦПЕРИОДА(жОстаткиИОбороты.ПериодСекунда, ДЕНЬ)
			ТОГДА ДОБАВИТЬКДАТЕ(жОстаткиИОбороты.ПериодСекунда, СЕКУНДА, -2)
		ИНАЧЕ
			жОстаткиИОбороты.ПериодСекунда
	КОНЕЦ КАК Секунда,
	жОстаткиИОбороты.ПериодМесяц КАК Месяц,
	жОстаткиИОбороты.ПериодКвартал КАК Квартал,
	жОстаткиИОбороты.ПериодГод КАК Год
ПОМЕСТИТЬ ОборотыИОстатки
ИЗ
	РегистрБухгалтерии.ЖурналОпераций.ОстаткиИОбороты(&amp;ДатаОтбора1 {(&amp;ДатаОтбора1)}, &amp;ДатаОтбора2 {(&amp;ДатаОтбора2)}, Авто, ДвиженияИГраницыПериода, Счет В ИЕРАРХИИ (ЗНАЧЕНИЕ(ПланСчетов.РазделыУчета.Деньги)), , {(Валюта).* КАК Валюта, (ВЫРАЗИТЬ(Субконто1 КАК Справочник.КошелькиИСчета)).* КАК Кошелек, (ВЫРАЗИТЬ(ЕСТЬNULL(Субконто2, НЕОПРЕДЕЛЕНО) КАК Справочник.ФинансовыеЦели)).* КАК ФинансоваяЦель}) КАК жОстаткиИОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК КурсыВалют
		ПО жОстаткиИОбороты.ПериодДень = КурсыВалют.Период
			И жОстаткиИОбороты.Валюта = КурсыВалют.Валюта
			И (КурсыВалют.БазоваяВалюта = &amp;ВалютаУчета)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК КурсыВалютыОтчета
		ПО (КурсыВалютыОтчета.Валюта = &amp;ВалютаОтчета)
			И (КурсыВалютыОтчета.БазоваяВалюта = &amp;ВалютаУчета)
			И жОстаткиИОбороты.ПериодДень = КурсыВалютыОтчета.Период
		ЛЕВОЕ СОЕДИНЕНИЕ ПоследниеКурсы КАК ПоследниеКурсы
		ПО жОстаткиИОбороты.Валюта = ПоследниеКурсы.Валюта
		ЛЕВОЕ СОЕДИНЕНИЕ ПоследниеКурсы КАК ПоследниеКурсыОтчета
		ПО (ПоследниеКурсыОтчета.Валюта = &amp;ВалютаОтчета)
		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаПериодов КАК ТаблицаПериодов
		ПО (жОстаткиИОбороты.ПериодСекунда МЕЖДУ ТаблицаПериодов.НачалоПериода И ТаблицаПериодов.ОкончаниеПериода)
{ГДЕ
	жОстаткиИОбороты.Регистратор.* КАК Операция,
	жОстаткиИОбороты.Валюта.*,
	(ВЫРАЗИТЬ(ЕСТЬNULL(жОстаткиИОбороты.Субконто2, НЕОПРЕДЕЛЕНО) КАК Справочник.ФинансовыеЦели)).* КАК ФинансоваяЦель,
	жОстаткиИОбороты.Счет.* КАК ВидДенег,
	(ВЫБОР
			КОГДА ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета).ТипСчета = ЗНАЧЕНИЕ(Перечисление.ТипыСчетов.БанковскаяКартаКредитная)
				ТОГДА &amp;Синоним_КредитныеКарты
			КОГДА ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета).ТипСчета = ЗНАЧЕНИЕ(Перечисление.ТипыСчетов.БанковскаяКартаУниверсальная)
				И (жОстаткиИОбороты.ВалютнаяСуммаКонечныйОстаток &lt; 0 ИЛИ жОстаткиИОбороты.ВалютнаяСуммаНачальныйОстаток &lt; 0)
				ТОГДА &amp;Синоним_КредитныеКарты
			ИНАЧЕ &amp;Синоним_ДеньгиВНаличии
		КОНЕЦ) КАК ТипКошелька,
	(ВЫРАЗИТЬ(жОстаткиИОбороты.Субконто1 КАК Справочник.КошелькиИСчета)).* КАК Кошелек}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Подзапрос.НомерПериода,
	Подзапрос.День,
	Подзапрос.Валюта
ПОМЕСТИТЬ ВсеПериоды
ИЗ
	(ВЫБРАТЬ
		Периоды.НомерПериода КАК НомерПериода,
		Периоды.День КАК День,
		Валюты.Ссылка КАК Валюта
	ИЗ
		(ВЫБРАТЬ
			ТаблицаПериодов.НомерПериода КАК НомерПериода,
			ТаблицаПериодов.НачалоПериода КАК День
		ИЗ
			ТаблицаПериодов КАК ТаблицаПериодов
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ
			ТаблицаПериодов.НомерПериода,
			НАЧАЛОПЕРИОДА(ТаблицаПериодов.ОкончаниеПериода, ДЕНЬ)
		ИЗ
			ТаблицаПериодов КАК ТаблицаПериодов) КАК Периоды
			ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Валюты КАК Валюты
			ПО (1 = 1)
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ РАЗЛИЧНЫЕ
		ТаблицаПериодов.НомерПериода,
		ОборотыИОстатки.День,
		ОборотыИОстатки.Валюта
	ИЗ
		ОборотыИОстатки КАК ОборотыИОстатки
			ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаПериодов КАК ТаблицаПериодов
			ПО (ОборотыИОстатки.Секунда МЕЖДУ ТаблицаПериодов.НачалоПериода И ТаблицаПериодов.ОкончаниеПериода)) КАК Подзапрос
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ТаблицаПериодов.НомерПериода КАК НомерПериода,
	НАЧАЛОПЕРИОДА(ТаблицаПериодов.День, ДЕНЬ) КАК День,
	ТаблицаПериодов.Валюта КАК Валюта,
	ВЫРАЗИТЬ(ЕСТЬNULL(КурсыВалют.Курс, ЕСТЬNULL(ПоследниеКурсы.Курс, 0)) * ЕСТЬNULL(КурсыВалютыОтчета.Кратность, ЕСТЬNULL(ПоследниеКурсыОтчета.Кратность, 0)) / ВЫБОР
			КОГДА ЕСТЬNULL(КурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(КурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1)) = 0
				ТОГДА 1
			ИНАЧЕ ЕСТЬNULL(КурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(КурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1))
		КОНЕЦ КАК ЧИСЛО(15, 8)) КАК Курс,
	ВЫРАЗИТЬ(ЕСТЬNULL(ПредыдущиеКурсыВалют.Курс, ЕСТЬNULL(ПоследниеКурсы.Курс, 0)) * ЕСТЬNULL(ПредыдущиеКурсыВалютыОтчета.Кратность, ЕСТЬNULL(ПоследниеКурсыОтчета.Кратность, 0)) / ВЫБОР
			КОГДА ЕСТЬNULL(ПредыдущиеКурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(ПредыдущиеКурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1)) = 0
				ТОГДА 1
			ИНАЧЕ ЕСТЬNULL(ПредыдущиеКурсыВалютыОтчета.Курс, ЕСТЬNULL(ПоследниеКурсыОтчета.Курс, 1)) * ЕСТЬNULL(ПредыдущиеКурсыВалют.Кратность, ЕСТЬNULL(ПоследниеКурсы.Кратность, 1))
		КОНЕЦ КАК ЧИСЛО(15, 8)) КАК ПредыдущийКурс
ПОМЕСТИТЬ КурсыВалютПоВсемПериодам
ИЗ
	ВсеПериоды КАК ТаблицаПериодов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК КурсыВалют
		ПО (НАЧАЛОПЕРИОДА(ТаблицаПериодов.День, ДЕНЬ) = КурсыВалют.Период)
			И ТаблицаПериодов.Валюта = КурсыВалют.Валюта
			И (КурсыВалют.БазоваяВалюта = &amp;ВалютаУчета)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК КурсыВалютыОтчета
		ПО (КурсыВалютыОтчета.Валюта = &amp;ВалютаОтчета)
			И (КурсыВалютыОтчета.БазоваяВалюта = &amp;ВалютаУчета)
			И (НАЧАЛОПЕРИОДА(ТаблицаПериодов.День, ДЕНЬ) = КурсыВалютыОтчета.Период)
		ЛЕВОЕ СОЕДИНЕНИЕ ПоследниеКурсы КАК ПоследниеКурсы
		ПО ТаблицаПериодов.Валюта = ПоследниеКурсы.Валюта
		ЛЕВОЕ СОЕДИНЕНИЕ ПоследниеКурсы КАК ПоследниеКурсыОтчета
		ПО (ПоследниеКурсыОтчета.Валюта = &amp;ВалютаОтчета)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК ПредыдущиеКурсыВалют
		ПО (НАЧАЛОПЕРИОДА(ДОБАВИТЬКДАТЕ(ТаблицаПериодов.День, ДЕНЬ, -1), ДЕНЬ) = ПредыдущиеКурсыВалют.Период)
			И ТаблицаПериодов.Валюта = ПредыдущиеКурсыВалют.Валюта
			И (ПредыдущиеКурсыВалют.БазоваяВалюта = &amp;ВалютаУчета)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КурсыВалют КАК ПредыдущиеКурсыВалютыОтчета
		ПО (ПредыдущиеКурсыВалютыОтчета.Валюта = &amp;ВалютаОтчета)
			И (НАЧАЛОПЕРИОДА(ДОБАВИТЬКДАТЕ(ТаблицаПериодов.День, ДЕНЬ, -1), ДЕНЬ) = ПредыдущиеКурсыВалютыОтчета.Период)
			И (ПредыдущиеКурсыВалютыОтчета.БазоваяВалюта = &amp;ВалютаУчета)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Подзапрос.НомерПериода,
	Подзапрос.День,
	Подзапрос.Валюта,
	Подзапрос.Курс,
	ЕСТЬNULL(ПредыдущиеКурсы.Курс, Подзапрос.ПредыдущийКурс) КАК ПредыдущийКурс
ПОМЕСТИТЬ КурсыВалютПоПериодам
ИЗ
	(ВЫБРАТЬ
		КурсыВалютПоВсемПериодам.НомерПериода КАК НомерПериода,
		КурсыВалютПоВсемПериодам.День КАК День,
		КурсыВалютПоВсемПериодам.Валюта КАК Валюта,
		КурсыВалютПоВсемПериодам.Курс КАК Курс,
		КурсыВалютПоВсемПериодам.ПредыдущийКурс КАК ПредыдущийКурс,
		МАКСИМУМ(ПредыдущиеКурсы.День) КАК ПредыдущийДень
	ИЗ
		КурсыВалютПоВсемПериодам КАК КурсыВалютПоВсемПериодам
			ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютПоВсемПериодам КАК ПредыдущиеКурсы
			ПО КурсыВалютПоВсемПериодам.Валюта = ПредыдущиеКурсы.Валюта
				И (ПредыдущиеКурсы.День &lt; КурсыВалютПоВсемПериодам.День)
	
	СГРУППИРОВАТЬ ПО
		КурсыВалютПоВсемПериодам.НомерПериода,
		КурсыВалютПоВсемПериодам.День,
		КурсыВалютПоВсемПериодам.Валюта,
		КурсыВалютПоВсемПериодам.Курс,
		КурсыВалютПоВсемПериодам.ПредыдущийКурс) КАК Подзапрос
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютПоВсемПериодам КАК ПредыдущиеКурсы
		ПО Подзапрос.Валюта = ПредыдущиеКурсы.Валюта
			И Подзапрос.ПредыдущийДень = ПредыдущиеКурсы.День
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СводОстатков.НомерПериода КАК НомерПериода,
	СводОстатков.День КАК День,
	СводОстатков.Валюта КАК Валюта,
	СводОстатков.Кошелек КАК Кошелек,
	СУММА(СводОстатков.НаНачалоВВалютеКошелька) КАК НаНачалоВВалютеКошелька,
	СУММА(СводОстатков.НаКонецВВалютеКошелька) КАК НаКонецВВалютеКошелька,
	СУММА(ВЫБОР
			КОГДА СводОстатков.День = НАЧАЛОПЕРИОДА(СводОстатков.День, ДЕНЬ)
				ТОГДА СводОстатков.НаНачалоВВалютеКошелька * КурсыВалютПоПериодам.Курс - СводОстатков.НаНачалоВВалютеКошелька * КурсыВалютПоПериодам.ПредыдущийКурс
			ИНАЧЕ 0
		КОНЕЦ) КАК ПереоценкаНаНачалоПериода,
	КурсыВалютПоПериодам.Курс,
	КурсыВалютПоПериодам.ПредыдущийКурс,
	СУММА(ВЫБОР
			КОГДА СводОстатков.День = НАЧАЛОПЕРИОДА(СводОстатков.День, ДЕНЬ)
				ТОГДА СводОстатков.НаНачалоВВалютеКошелька * КурсыВалютПоПериодам.ПредыдущийКурс
			ИНАЧЕ СводОстатков.НаНачалоВВалютеКошелька * ЕСТЬNULL(КурсыВалютПоПериодам.Курс, 0)
		КОНЕЦ) КАК НаНачало,
	СУММА(СводОстатков.НаКонецВВалютеКошелька * ЕСТЬNULL(КурсыВалютПоПериодам.Курс, 0)) КАК НаКонец,
	СводОстатков.ФинансоваяЦель КАК ФинансоваяЦель,
	СводОстатков.ВидДенег КАК ВидДенег,
	СводОстатков.ТипКошелька КАК ТипКошелька,
	СводОстатков.СимволВалюты КАК СимволВалюты,
	СводОстатков.ЭтоГруппаКошельков КАК ЭтоГруппаКошельков
ПОМЕСТИТЬ ОстаткиПоПериодам
ИЗ
	(ВЫБРАТЬ
		ДатыОстатков.НомерПериода КАК НомерПериода,
		ДатыОстатков.День КАК День,
		ДатыОстатков.Валюта КАК Валюта,
		ДатыОстатков.Кошелек КАК Кошелек,
		ВЫБОР
			КОГДА ДатыОстатков.СекундаОстатка &lt; ДатыОстатков.День
				ТОГДА ОборотыИОстатки.НаКонецВВалютеКошелька
			ИНАЧЕ ОборотыИОстатки.НаНачалоВВалютеКошелька
		КОНЕЦ КАК НаНачалоВВалютеКошелька,
		0 КАК НаКонецВВалютеКошелька,
		ДатыОстатков.ФинансоваяЦель КАК ФинансоваяЦель,
		ОборотыИОстатки.ВидДенег КАК ВидДенег,
		ОборотыИОстатки.ТипКошелька КАК ТипКошелька,
		ОборотыИОстатки.СимволВалюты КАК СимволВалюты,
		ОборотыИОстатки.ЭтоГруппаКошельков КАК ЭтоГруппаКошельков
	ИЗ
		(ВЫБРАТЬ
			ВсеПериоды.НомерПериода КАК НомерПериода,
			ВсеПериоды.День КАК День,
			МАКСИМУМ(ОборотыИОстатки.Секунда) КАК СекундаОстатка,
			ОборотыИОстатки.Валюта КАК Валюта,
			ОборотыИОстатки.Кошелек КАК Кошелек,
			ОборотыИОстатки.ФинансоваяЦель КАК ФинансоваяЦель
		ИЗ
			ВсеПериоды КАК ВсеПериоды
				ЛЕВОЕ СОЕДИНЕНИЕ ОборотыИОстатки КАК ОборотыИОстатки
				ПО (ОборотыИОстатки.Секунда &lt;= ВсеПериоды.День)
					И (ОборотыИОстатки.Валюта = ВсеПериоды.Валюта)
		
		СГРУППИРОВАТЬ ПО
			ВсеПериоды.НомерПериода,
			ВсеПериоды.День,
			ОборотыИОстатки.Валюта,
			ОборотыИОстатки.Кошелек,
			ОборотыИОстатки.ФинансоваяЦель) КАК ДатыОстатков
			ЛЕВОЕ СОЕДИНЕНИЕ ОборотыИОстатки КАК ОборотыИОстатки
			ПО ДатыОстатков.Валюта = ОборотыИОстатки.Валюта
				И ДатыОстатков.СекундаОстатка = ОборотыИОстатки.Секунда
				И ДатыОстатков.Кошелек = ОборотыИОстатки.Кошелек
				И (ЕСТЬNULL(ДатыОстатков.ФинансоваяЦель, НЕОПРЕДЕЛЕНО) = ЕСТЬNULL(ОборотыИОстатки.ФинансоваяЦель, НЕОПРЕДЕЛЕНО))
	{ГДЕ
		ДатыОстатков.Валюта.*,
		ДатыОстатков.Кошелек.*,
		ДатыОстатков.ФинансоваяЦель.*,
		ОборотыИОстатки.ВидДенег.*,
		ОборотыИОстатки.ТипКошелька}
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ДатыОстатков.НомерПериода,
		ДатыОстатков.День,
		ДатыОстатков.Валюта,
		ДатыОстатков.Кошелек,
		0,
		ВЫБОР
			КОГДА ДатыОстатков.СекундаОстатка &gt; ДатыОстатков.День
				ТОГДА ОборотыИОстатки.НаНачалоВВалютеКошелька
			ИНАЧЕ ОборотыИОстатки.НаКонецВВалютеКошелька
		КОНЕЦ,
		ДатыОстатков.ФинансоваяЦель,
		ОборотыИОстатки.ВидДенег,
		ОборотыИОстатки.ТипКошелька,
		ОборотыИОстатки.СимволВалюты,
		ОборотыИОстатки.ЭтоГруппаКошельков
	ИЗ
		(ВЫБРАТЬ
			ВсеПериоды.НомерПериода КАК НомерПериода,
			ВсеПериоды.День КАК День,
			МАКСИМУМ(ОборотыИОстатки.Секунда) КАК СекундаОстатка,
			ОборотыИОстатки.Валюта КАК Валюта,
			ОборотыИОстатки.Кошелек КАК Кошелек,
			ОборотыИОстатки.ФинансоваяЦель КАК ФинансоваяЦель
		ИЗ
			ВсеПериоды КАК ВсеПериоды
				ЛЕВОЕ СОЕДИНЕНИЕ ОборотыИОстатки КАК ОборотыИОстатки
				ПО (ОборотыИОстатки.Секунда &lt;= ВсеПериоды.День)
					И (ОборотыИОстатки.Валюта = ВсеПериоды.Валюта)
		
		СГРУППИРОВАТЬ ПО
			ВсеПериоды.НомерПериода,
			ВсеПериоды.День,
			ОборотыИОстатки.Валюта,
			ОборотыИОстатки.Кошелек,
			ОборотыИОстатки.ФинансоваяЦель) КАК ДатыОстатков
			ЛЕВОЕ СОЕДИНЕНИЕ ОборотыИОстатки КАК ОборотыИОстатки
			ПО ДатыОстатков.Валюта = ОборотыИОстатки.Валюта
				И ДатыОстатков.СекундаОстатка = ОборотыИОстатки.Секунда
				И ДатыОстатков.Кошелек = ОборотыИОстатки.Кошелек
				И (ЕСТЬNULL(ДатыОстатков.ФинансоваяЦель, НЕОПРЕДЕЛЕНО) = ЕСТЬNULL(ОборотыИОстатки.ФинансоваяЦель, НЕОПРЕДЕЛЕНО))
	{ГДЕ
		ДатыОстатков.Валюта.*,
		ДатыОстатков.Кошелек.*,
		ДатыОстатков.ФинансоваяЦель.*,
		ОборотыИОстатки.ВидДенег.*,
		ОборотыИОстатки.ТипКошелька}) КАК СводОстатков
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютПоПериодам КАК КурсыВалютПоПериодам
		ПО СводОстатков.Валюта = КурсыВалютПоПериодам.Валюта
			И (НАЧАЛОПЕРИОДА(СводОстатков.День, ДЕНЬ) = НАЧАЛОПЕРИОДА(КурсыВалютПоПериодам.День, ДЕНЬ))
ГДЕ
	НЕ КурсыВалютПоПериодам.Курс ЕСТЬ NULL 

СГРУППИРОВАТЬ ПО
	СводОстатков.НомерПериода,
	СводОстатков.День,
	СводОстатков.Валюта,
	СводОстатков.Кошелек,
	СводОстатков.ФинансоваяЦель,
	СводОстатков.ВидДенег,
	СводОстатков.ТипКошелька,
	СводОстатков.СимволВалюты,
	СводОстатков.ЭтоГруппаКошельков,
	КурсыВалютПоПериодам.Курс,
	КурсыВалютПоПериодам.ПредыдущийКурс
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ОборотыИОстатки.Кошелек, ОстаткиПоПериодам.Кошелек) КАК Кошелек,
	ЕСТЬNULL(ОборотыИОстатки.Кошелек.Активность, ОстаткиПоПериодам.Кошелек.Активность) КАК КошелекАктуален,
	ЕСТЬNULL(ОборотыИОстатки.Валюта, ОстаткиПоПериодам.Валюта) КАК Валюта,
	ЕСТЬNULL(ОстаткиПоПериодам.НомерПериода, ОборотыИОстатки.НомерПериода) КАК НомерПериода,
	ЕСТЬNULL(ОборотыИОстатки.Секунда, ОстаткиПоПериодам.День) КАК Секунда,
	ЕСТЬNULL(ОстаткиПоПериодам.НаНачало, ОборотыИОстатки.НаНачалоВВалютеКошелька * ОборотыИОстатки.Курс) КАК НаНачало,
	ЕСТЬNULL(ОборотыИОстатки.ПриходВВалютеКошелька * ОборотыИОстатки.Курс, 0) КАК Приход,
	ЕСТЬNULL(ОборотыИОстатки.РасходВВалютеКошелька * ОборотыИОстатки.Курс, 0) КАК Расход,
	ЕСТЬNULL(ОстаткиПоПериодам.ПереоценкаНаНачалоПериода, 0) КАК КурсоваяРазница,
	ЕСТЬNULL(ОстаткиПоПериодам.НаКонец, ОборотыИОстатки.НаКонецВВалютеКошелька * ОборотыИОстатки.Курс) КАК НаКонец,
	ЕСТЬNULL(ОборотыИОстатки.Курс, ОстаткиПоПериодам.Курс) КАК Курс,
	ЕСТЬNULL(ОстаткиПоПериодам.ПредыдущийКурс, ОборотыИОстатки.Курс) КАК ПредыдущийКурс,
	ЕСТЬNULL(ОборотыИОстатки.НаНачалоВВалютеКошелька, ОстаткиПоПериодам.НаНачалоВВалютеКошелька) КАК НаНачалоВВалютеКошелька,
	ЕСТЬNULL(ОборотыИОстатки.ПриходВВалютеКошелька, 0) КАК ПриходВВалютеКошелька,
	ЕСТЬNULL(ОборотыИОстатки.РасходВВалютеКошелька, 0) КАК РасходВВалютеКошелька,
	ЕСТЬNULL(ОборотыИОстатки.НаКонецВВалютеКошелька, ОстаткиПоПериодам.НаКонецВВалютеКошелька) КАК НаКонецВВалютеКошелька,
	ЕСТЬNULL(ОстаткиПоПериодам.ПереоценкаНаНачалоПериода, 0) КАК ПереоценкаНаНачалоПериода,
	ЕСТЬNULL(ОборотыИОстатки.ФинансоваяЦель, ОстаткиПоПериодам.ФинансоваяЦель) КАК ФинансоваяЦель,
	ЕСТЬNULL(ОборотыИОстатки.ВидДенег, ОстаткиПоПериодам.ВидДенег) КАК ВидДенег,
	ЕСТЬNULL(ОборотыИОстатки.ТипКошелька, ОстаткиПоПериодам.ТипКошелька) КАК ТипКошелька,
	ЕСТЬNULL(ОборотыИОстатки.СимволВалюты, ОстаткиПоПериодам.СимволВалюты) КАК СимволВалюты,
	ЕСТЬNULL(ОборотыИОстатки.ЭтоГруппаКошельков, ОстаткиПоПериодам.ЭтоГруппаКошельков) КАК ЭтоГруппаКошельков,
	НАЧАЛОПЕРИОДА(ЕСТЬNULL(ОборотыИОстатки.День, ОстаткиПоПериодам.День), ДЕНЬ) КАК День,
	НАЧАЛОПЕРИОДА(ЕСТЬNULL(ОборотыИОстатки.Месяц, ОстаткиПоПериодам.День), МЕСЯЦ) КАК Месяц,
	НАЧАЛОПЕРИОДА(ЕСТЬNULL(ОборотыИОстатки.Квартал, ОстаткиПоПериодам.День), КВАРТАЛ) КАК Квартал,
	НАЧАЛОПЕРИОДА(ЕСТЬNULL(ОборотыИОстатки.Год, ОстаткиПоПериодам.День), ГОД) КАК Год,
	ЕСТЬNULL(ОборотыИОстатки.Операция, НЕОПРЕДЕЛЕНО) КАК Операция,
	ЕСТЬNULL(ОборотыИОстатки.ОписаниеОперации, &amp;Синоним_Остаток) КАК ОписаниеОперации
{ВЫБРАТЬ
	Кошелек.*,
	КошелекАктуален,
	Валюта.*,
	НомерПериода,
	Секунда,
	НаНачало,
	Приход,
	Расход,
	КурсоваяРазница,
	НаКонец,
	Курс,
	ПредыдущийКурс,
	НаНачалоВВалютеКошелька,
	ПриходВВалютеКошелька,
	РасходВВалютеКошелька,
	НаКонецВВалютеКошелька,
	ПереоценкаНаНачалоПериода,
	ФинансоваяЦель.*,
	ВидДенег.*,
	ТипКошелька,
	СимволВалюты,
	ЭтоГруппаКошельков,
	День,
	Месяц,
	Квартал,
	Год,
	Операция.*,
	ОписаниеОперации}
ИЗ
	ОстаткиПоПериодам КАК ОстаткиПоПериодам
		ПОЛНОЕ СОЕДИНЕНИЕ ОборотыИОстатки КАК ОборотыИОстатки
		ПО ОстаткиПоПериодам.День = ОборотыИОстатки.Секунда
			И ОстаткиПоПериодам.Валюта = ОборотыИОстатки.Валюта
			И ОстаткиПоПериодам.Кошелек = ОборотыИОстатки.Кошелек
			И (ЕСТЬNULL(ОстаткиПоПериодам.ФинансоваяЦель, НЕОПРЕДЕЛЕНО) = ЕСТЬNULL(ОборотыИОстатки.ФинансоваяЦель, НЕОПРЕДЕЛЕНО))
{ГДЕ
	(ЕСТЬNULL(ОборотыИОстатки.Операция, НЕОПРЕДЕЛЕНО)).* КАК Операция,
	(ЕСТЬNULL(ОборотыИОстатки.ОписаниеОперации, &amp;Синоним_Остаток)) КАК ОписаниеОперации,
	(ЕСТЬNULL(ОборотыИОстатки.Кошелек.Активность, ОстаткиПоПериодам.Кошелек.Активность)) КАК КошелекАктуален}

